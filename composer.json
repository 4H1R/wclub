{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "calebporzio/sushi": "^2.5",
    "codewithdennis/filament-select-tree": "^3.1",
    "dotswan/filament-map-picker": "^1.7",
    "emreyarligan/enum-concern": "^1.0",
    "filament/filament": "^3.2",
    "filament/spatie-laravel-media-library-plugin": "^3.2",
    "filament/spatie-laravel-settings-plugin": "^3.2",
    "hekmatinasser/verta": "^8.4",
    "inertiajs/inertia-laravel": "^2.0",
    "kasraghoreyshi/persian-faker": "^1.04",
    "laravel/framework": "^12.29.0",
    "laravel/horizon": "^5.29",
    "laravel/octane": "^2.12.2",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.9",
    "league/flysystem-aws-s3-v3": "^3.0",
    "mokhosh/filament-jalali": "^4.4",
    "opcodesio/log-viewer": "^3.12",
    "rawilk/filament-password-input": "^2.0",
    "spatie/eloquent-sortable": "^4.4",
    "spatie/laravel-data": "^4.11",
    "spatie/laravel-honeypot": "^4.5",
    "spatie/laravel-medialibrary": "^11.10",
    "spatie/laravel-permission": "^6.10",
    "spatie/laravel-query-builder": "^6.2",
    "spatie/laravel-responsecache": "^7.6",
    "spatie/laravel-settings": "^3.4",
    "spatie/laravel-typescript-transformer": "^2.5",
    "spatie/typescript-transformer": "^2.4",
    "tightenco/ziggy": "^2.4",
    "vinkla/hashids": "^13.0.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^3.2",
    "fakerphp/faker": "^1.23",
    "larastan/larastan": "^3.0",
    "laravel/boost": "^1.1",
    "laravel/breeze": "^2.2",
    "laravel/pail": "^1.1",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "laravel/telescope": "^5.2",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.1",
    "pestphp/pest": "^4.1.0",
    "pestphp/pest-plugin-laravel": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ],
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
